<!-- For IDs just one selector that only works for one element with that specific ID -->

<!-- div p {} covers every paragraph inside of div -->

<!-- CSS should live in a separate file -->

<!doctype html>
<html class="no-js" lang="en">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="x-ua-compatible" content="ie=edge">
        <title>Page Name</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">


        <!-- External link (relative) to your main stylesheet -->
        <link rel="stylesheet" href="css/screen.css">

        <!-- External link (relative) to your javascript file -->
        <script src="js/index.js"></script>
    </head>
    <body>
        <!-- Add your site content here -->
        <!-- Relative URL image link example -->
        <img src="images/northport.jpg" alt="Northport" /> <br>

    </body>
</html>

